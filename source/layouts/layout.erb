<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    
    <!-- Always force latest IE rendering engine or request Chrome Frame -->
    <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">
    
    <meta name="viewport" content="width=device-width, user-scalable=no">

    <!-- Use title if it's in the page YAML frontmatter -->
    <title><%= current_page.data.title || "TakeOff" %></title>
    
    <%= stylesheet_link_tag "all" %>
    <script src="//ajax.googleapis.com/ajax/libs/webfont/1.5.2/webfont.js"></script>
    <script>
      WebFont.load({
        google: {
          families: ['Dosis', 'Sorts Mill Goudy']
        }
      });
    </script>
  </head>
  
  <body class="<%= page_classes %>">
    <%= yield %>
    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js"></script>
    <script src="http://cdnjs.cloudflare.com/ajax/libs/gsap/1.11.5/TweenMax.min.js"></script>
    <%= javascript_include_tag  "all" %>
  </body>
</html>